{
  "name": "labs-standard-errors",
  "version": "1.0.0",
  "description": "LuizaLabs Standard API Errors",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && nyc npm run test-mocha",
    "test-mocha": "NODE_CONFIG_DIR=./src/config NODE_ENV=test mocha --harmony --exit ./__tests__/*.js",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s && npm run test -s",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luanpablo-luizalabs/labs-standard-errors.git"
  },
  "keywords": [
    "API",
    "Errors",
    "standard",
    "errors",
    "LuizaLabs"
  ],
  "author": "Luan Pablo, CÃ¡ssio Thadeu",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/luanpablo-luizalabs/labs-standard-errors/issues"
  },
  "homepage": "https://github.com/luanpablo-luizalabs/labs-standard-errors#readme",
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-eslint": "^8.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "husky": "^0.14.3",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1",
    "should": "^13.2.1",
    "sinon": "^4.2.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.20.1"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "test",
      "beforeAll"
    ]
  },
  "nyc": {
    "include": [
      "apigee/**/*.js",
      "apigee/**/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": false
  }
}
